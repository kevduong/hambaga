	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

	<title>HAMBAGA</title>

	<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">

	<!--     Fonts and icons     -->
	<link rel="stylesheet" type="text/css" href="./assets/material-bootstrap/css">
	<link rel="stylesheet" href="./assets/material-bootstrap/font-awesome.min.css">

	<!-- CSS Files -->
	<link href="./assets/material-bootstrap/bootstrap.min.css" rel="stylesheet">
	<link href="./assets/material-bootstrap/material-kit.css" rel="stylesheet">
	<script type="text/javascript" charset="UTF-8" src="./assets/material-bootstrap/common.js"></script>
	<script type="text/javascript" charset="UTF-8" src="./assets/material-bootstrap/util.js"></script>
	<script type="text/javascript" charset="UTF-8" src="./assets/material-bootstrap/stats.js"></script>
</head>

<body class="blog-post">



	<div class="page-header header-filter" data-parallax="active" style="background-image: url(&quot;burger-header.gif&quot;); transform: translate3d(0px, 15.3333px, 0px);">
		<div class="container">
			<div class="row">
				<div class="col-md-8 col-md-offset-2 text-center">
					<h1 class="title">HAMBAGA</h1>
					<br>
					<h4>track all the burgers you always wanted to try</h4>
					<br>
					<h4> and save the ones that you devoured!</h4>
					<br>

				</div>
			</div>
		</div>
	</div>

	<div class="main main-raised">
		<div class="container">
			<div class="section section-text">
				<div class="row">
					<div class="col-md-8 col-md-offset-2">




					</div>




				</div>
			</div>



			<div class="section section-comments">
				<div class="row">
					<div class="col-md-8 col-md-offset-2">
						<div class="media-area">
							<h3 class="title text-center">Burgers</h3>
							<div class="media">
								<!-- insert media -->
								<h2>DEVOURED BURGERS</h2>
								<ul>
								  {{#each burgers}}
								    {{#unless this.devoured}}
								      <li>
								        {{this.burger_name}}

								        <form action="/{{this.id}}?_method=PUT" method="POST">
								          <input type="hidden" name="devoured" value="true">
								          <button type="submit">DEVOURED</button>
								        </form>

								        {{> burger/delete-cat}}
								      </li>
								    {{/unless}}
								  {{/each}}
								</ul>

								<h2>BURGER MENU</h2>

								<ul>
								  {{#each burgers}}
								    {{#if this.devoured}}
								      <li>
								        {{this.burger_name}}

								        <form action="/{{this.id}}?_method=PUT" method="POST">
								          <input type="hidden" name="devoured" value="false">
								          <button type="submit">DEVOUR!</button>
								        </form>

								        {{> burger/delete-cat}}
								      </li>
								    {{/if}}
								  {{/each}}
								</ul>

								<h2>Add a Burger</h2>
								<form class="create-update-form" action="/" method="POST">

								  <div class="form-group">
								    <label for="ca">Burgers Name:</label>
								    <input type="text" id="ca" name="name">
								  </div>

								  <button type="submit">Add Burger</button>
								</form>

							</div>




						</div>
					</div>

				</div>

			</div>
		</div>
	</div>


	<div class="section">

	</div>

	<footer class="footer footer-black">
		<div class="container">
			<a class="footer-brand" href="file:///Volumes/Hard%20Drive/Design/UI%20Kit/Google%20Material%20Template/HTML/examples/blog-post.html#pablo">2017 Kevin Duong</a>



			<ul class="social-buttons pull-right">
				<li>
					<a href="https://twitter.com/CreativeTim" target="_blank" class="btn btn-just-icon btn-simple">
						<i class="fa fa-twitter"></i>
					</a>
				</li>
				<li>
					<a href="https://www.facebook.com/CreativeTim" target="_blank" class="btn btn-just-icon btn-simple">
						<i class="fa fa-facebook-square"></i>
					</a>
				</li>
				<li>
					<a href="https://www.instagram.com/CreativeTimOfficial" target="_blank" class="btn btn-just-icon btn-simple">
						<i class="fa fa-instagram"></i>
					</a>
				</li>
			</ul>

		</div>
	</footer>


	<!--   Core JS Files   -->
	<script src="./assets/material-bootstrap/jquery.min.js" type="text/javascript"></script>
	<script src="./assets/material-bootstrap/bootstrap.min.js" type="text/javascript"></script>
	<script src="./assets/material-bootstrap/material.min.js"></script>

	<!--	Plugin for the Sliders, full documentation here: http://refreshless.com/nouislider/ -->
	<script src="./assets/material-bootstrap/nouislider.min.js" type="text/javascript"></script>

	<!--	Plugin for the Datepicker, full documentation here: http://www.eyecon.ro/bootstrap-datepicker/ -->
	<script src="./assets/material-bootstrap/bootstrap-datepicker.js" type="text/javascript"></script>

	<!--	Plugin for Select Form control, full documentation here: https://github.com/FezVrasta/dropdown.js -->
